<Project>
  <!-- The ChainSharp Dashboard uses Blazor Server and requires the ASP.NET Core framework web assets
       (blazor.web.js) to be resolved at build time. Without this, _framework/blazor.web.js returns 404.
       This is in a .targets file (not .props) because the Razor SDK overwrites the .props file during pack. -->
  <PropertyGroup>
    <RequiresAspNetWebAssets Condition="'$(RequiresAspNetWebAssets)' == ''">true</RequiresAspNetWebAssets>
  </PropertyGroup>
</Project>
